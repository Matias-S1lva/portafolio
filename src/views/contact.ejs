<div id="list-item-3"
  class="border-top container d-flex justify-content-center align-items-center"
>
  <div class="card bg-dark text-center" style="margin: 32px 0">
    <div class="card-header">
      <h3>Contact Me</h3>
    </div>
    <div class="card-body text-start">
      <form
        action="https://formsubmit.co/3cb880a57f30d2ed9c2010285389e0d9"
        method="POST"
      >
        <div class="form-container d-flex justify-content-between flex-column">
          <div class="">
            <label for="email">Email</label>
            <input
            id="email"
              type="email"
              placeholder="email"
              name="email"
              class="form-control"
              style="background: #202020; color: white"
            />
          </div>

          <div class="">
            <label for="message">Message:</label>
            <textarea
              name="message"
              rows="3"
              class="form-control"
              placeholder="Write a Message"
              style="background: #202020; color: white"
            ></textarea>
          </div>
        </div>
        <button class="btn btn-primary btn-block mt-4">Send</button>
      </form>
    </div>
  </div>
</div>
<%- include('partials/footer')%>
